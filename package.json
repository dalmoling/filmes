  "dependencies": {
    "axios": "^1.9.0",
    "expo": "^52.0.46",
    "expo-router": "^4.0.20",
    "metro": "^0.81.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0"
  },
  "devDependencies": {
